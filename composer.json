{
  "name": "divante/product-alert",
  "description": "Product alert plugin with out of stock notification feature",
  "version": "1.0.0",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "keywords": ["divante", "out of stock notification", "notification", "product"],
  "require": {
    "shopware/core": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Divante\\ProductAlert\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Divante\\ProductAlert\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Divante LTD"
    },
    {
      "name": "Marek Mularczyk",
      "email": "mmularczyk9@gmail.com"
    }
  ],
  "extra": {
    "shopware-plugin-class": "Divante\\ProductAlert\\DivanteProductAlert",
    "label": {
      "en-GB": "Out of stock notification"
    },
    "description": {
      "en-GB": "Plugin allows customers to subscribe notification when product will back to stock."
    }
  }
}
